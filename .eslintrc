{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    //jsx-runtime tells eslint that we using
    //react 18/latest so please dont throw err
    "plugin:react/jsx-runtime",
    "prettier"
  ],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": "warn"
  }
}
